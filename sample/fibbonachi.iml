prog fibbonachi (in n:int, out o:int)
{
    def fib(in n :int, out o :int) {
        if(n = 0) 
        {
            o := 0;
        }
        else 
        {
            if(n = 1) 
            {
                o:= 1;
            }
            else
            {
                var a :int;
                var b :int;
                n := n - 1; 
                fib(n, a);
                n := n - 1; 
                fib(n, b);
                o := a + b;
            }
        }
    }

    fib(n, o);
}

