prog negate (out o: int) {

    def lala(in m: int, in n : int, out o: int) {
       o := m;
       while (m < 3) {
           if (m = 2) {
               o := o + 10;
           } else {
               o := o * 2;
           }
           m := m + 1;
       }
    }

    // method not used
    def myPen(in m: int, in n: int, out o: int) {
        o := m + n;
    }

    lala(0, 0, o);
    
}
